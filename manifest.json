{
	"manifest_version": 2,
	"name": "imgur++",
	"description": "Adds new features to imgur. Also changes the loading gif for imgur...",
	"version": "0.1",
	"permissions": [
		"webRequest",
		"webRequestBlocking",
		"*://*.imgur.com/*"
	],
	"content_scripts": 
	[
		{
		"matches":["*://imgur.com/*"],
		"js":["jquery-2.1.3.min.js", "resizer.js"],
		"css": ["inject.css"],
		"run_at": "document_end"
		}
	],
	"background": {"scripts": ["background.js"]}
}